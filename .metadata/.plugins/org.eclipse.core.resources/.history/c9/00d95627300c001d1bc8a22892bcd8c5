package helloLearn;

public class Asessment1 {

	public static void main(String[] args) {
		int []count = {1,2,3,4,5};
		
		for(int i=0; i<count.length; i++) {
			if(count[i]%2==0) {
				System.out.println("Even number: " + count[i]);
			}
			else {
				System.out.println("Odd number: " + count[i]);
			}
		}

}}
