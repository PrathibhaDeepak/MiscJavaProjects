package arrayPack;

import java.util.Scanner;

public class FahToDegree {

	public static void main(String[] args) {
		int temp;
		int cel;
		int fahr = 0;
		
		System.out.print("Enter temperature: ");
		Scanner input = new Scanner(System.in);
		temp = input.nextInt();
		input.close();
		
		cel = (5/9)*(fahr-32);
		
		System.out.printf("%d temp in celsius: ", cel);
	}

}
