package javaScanner;

import java.util.Scanner;

public class SumArrayInput {

	@SuppressWarnings("null")
	public static void main(String[] args) {
		int val;
		int i;
		int limit[] = null;
		Scanner input = new Scanner(System.in);
		System.out.println("Enter limit: \n");		
		val = input.nextInt();
		
		for(i=0; i<=val; i++) {
			System.out.printf("%d", limit[i]);
		}
		//System.out.printf("Entered values are:  \n",val);

		

	}

}
