package arrayPack;

public class BreakContinue {

	public static void main(String[] args) {
		for(int i=0; i<=10; i++)
		{System.out.print("\nHi");
			if(i==2) {
				break;}
			System.out.print("\nHello");
				
			}
		System.out.print("Program completed\n ");
	
	 	for(int i=1; i<=5; i++)
	 	{	for(int j=1; j<=5; j++)
	 		System.out.printf(" One ");
	 		 if(i==2)
	 		 	{continue;}
	 	System.out.println(" Two2 " );}
	 	
	 	System.out.println("2nd Program completed here\n ");
	 	int row =5;
	 	int min = 0;
	 	
	 	for(int i=0; i<row; i++ ) {
	 		System.out.print("*");
	 	}
	 	if(row==5) {
	 		min = min-1;
	 		System.out.print(row-1);
	 	}
	 	
	 	
	 		
	}
	

}