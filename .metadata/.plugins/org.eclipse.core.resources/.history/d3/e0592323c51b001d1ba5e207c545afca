package arrayPack;

public class AssignmentBroto {

	public static void main(String[] args) {
		int num=5;
		for(int i=1; i<=num;i++) {
			for(int j=1; j<=i; j++) {
				System.out.print(j);
			}
		System.out.print("\n");
		}
		System.out.print("\n2nd Assignment\n\n");
		int limit = 10;
		int sum=0;
		for(int k=1; k<=limit; k++) {
			
			
			limit = limit+k;
			
			if(limit%2==1) {
				System.out.printf("\nOdd Sum: %d", limit);
			}
			/*else {
				System.out.printf("\nEven: %d", k);
			}*/
			
		}
		
		
		
	
	}

}
