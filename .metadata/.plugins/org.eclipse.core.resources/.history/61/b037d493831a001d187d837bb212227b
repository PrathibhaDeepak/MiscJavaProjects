package arrayPack;

public class BreakContinue {

	public static void main(String[] args) {
		for(int i=0; i<=10; i++)
		{System.out.print("\nHi");
			if(i==2) {
				break;}
			System.out.print("\nHello");
				
			}
		System.out.print("Program completed\n");
	
	 	for(int i=0; i<=10; i++)
	 	{	for(int j=0; j<=7; j++)
	 		System.out.printf(" One ", j);
	 		if(i==3)
	 		{continue;}
	 	System.out.println(" Two2 " );}
	 	
	 	System.out.println("2nd Program completed here");
	 		
	}
	

}