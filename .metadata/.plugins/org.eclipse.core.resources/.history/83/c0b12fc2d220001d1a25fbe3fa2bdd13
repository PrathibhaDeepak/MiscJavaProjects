package javaScanner;

import java.util.Scanner;

public class ScannerJavaOne {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("What is your name?");
		String name = input.nextLine();
		
		System.out.println("Hello \t" + name);
		
		System.out.println("What is your age? \t");
		int age = input.nextInt();
		input.nextLine();
		System.out.println("You are " + age + "years old\n");
		
		
		System.out.println("Which is your favourite car?");
		String car = input.nextLine();
		System.out.printf("Favourite car is  "+ car);
				


	}

}
