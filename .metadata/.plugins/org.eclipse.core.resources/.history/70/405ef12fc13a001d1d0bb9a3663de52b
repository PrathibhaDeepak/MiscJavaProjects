package thisK;

public class ClassB extends ClassA {
	int one;
	ClassB(){
		System.out.println("It is   constructor B");
	}
	
	void hello() {
		System.out.println("This is class B");}
		
	void hello1(){
		
		super.display();
		one =10;
		super.one =75;
		int c = one+super.one;
		System.out.println(c);
		}
		
		
	
	
		
		
		
	

}
