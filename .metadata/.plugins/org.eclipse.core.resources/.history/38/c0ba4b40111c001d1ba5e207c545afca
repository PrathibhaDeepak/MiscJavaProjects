package arrayPack;

public class Pat4Assignment {

	public static void main(String[] args) {
System.out.print("\n\n 4- Pattern Assignment\t\n");
		
		int star=5;
		for(int x=1; x<=star; x++) {
			for(int y=1; y<=x; y++) {
			System.out.print("* ");
			}
			System.out.println();	
		}
		for(int i=1; i<=star; i++) {
			for(int j=star-i; j>=0; j--) {
				System.out.print("* ");	
			}
			System.out.println();	
		}
		
		
		
		
		
		

	}

}
