package JavaClassHindi;

public class hindiClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Student mark = new Student(); //mark is called object or instance
		mark.getId();
		mark.getName();
		mark.getName();
		
		Student tom = new Student();
		tom.id = 2;
		tom.name = "Tom";
		tom.age = 14;
		
		System.out.println(mark.name + " is " + mark.age + " years old. ");
		System.out.println(tom.name + " is " + tom.age + " years old. ");
		

		
	}

}
