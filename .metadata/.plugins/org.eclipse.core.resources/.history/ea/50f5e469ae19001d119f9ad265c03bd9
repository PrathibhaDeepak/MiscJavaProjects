package sampleJava;

public class PrimeOrNot {
// Prime number is that divisible by 1 and itself
	//Ex. 2,3,5,7,11,13
	public static void main(String[] args) {
		int num =7;
		int count =0;
		
		for(int i=1; i<=num; i++) {
			if(num%i ==0) {
				count++;
			}
		}
		if(count==2) {
			System.out.println("Prime Number");
		}
		else {
			System.out.printf("%d is not a Prime number");
		}
	}

}
