package arrayPack;

public class BreakContinue {

	public static void main(String[] args) {
		for(int i=0; i<=10; i++)
		{System.out.print("\nHi");
			if(i==2) {
				break;}
			System.out.print("\nHello");
				
			}
		System.out.print("Program completed\n ");
	
	 	for(int i=1; i<=5; i++)
	 	{	for(int j=1; j<=5; j++)
	 		System.out.printf(" One ");
	 		 if(i==2)
	 		 	{continue;}
	 	System.out.println(" Two2 " );}
	 	
	 	System.out.println("2nd Program completed here\n ");
	 	
	 	int row = 5;
	 	for(int i=0; i<row; i++) {
	 		for(int j=0; j<i; j++) {
	 			System.out.print(" ");
	 		}
	 		System.out.print("* ");
	 	
	 		for(int k=5-i; k>0; k--) {
	 			System.out.print("*");
	 	}
	 	}
	 	System.out.print();
	 	
	 		
	}
	

}