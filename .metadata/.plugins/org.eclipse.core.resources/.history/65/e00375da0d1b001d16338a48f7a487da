package arrayPack;

import java.util.Scanner;

public class Table5 {

	public static void main(String[] args) {
		int num;
		int sum = 0;
		System.out.print("Enter a number: ");
		Scanner input = new Scanner(System.in);
		 num = input.nextInt();
		 input.close();
		 for(int i=1; i<=num; i++) {
			 for(int j=1; j<=i; j++) {
				 sum = sum*i;
			 }
			 
			 System.out.print("\n%d * %d = %d"+ sum);
		 }
		

	}

}
