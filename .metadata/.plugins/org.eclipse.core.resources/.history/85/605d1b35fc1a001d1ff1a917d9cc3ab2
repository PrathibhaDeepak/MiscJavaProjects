package arrayPack;

public class PracPat {

	public static void main(String[] args) {
		int num=5;
		for(int i=0; i<=5;i++) {
			for(int j=0; j<=i; j++) {
			System.out.print("*");}
			
		System.out.print("\n");	
		}
		int row=10;
		for(int k=row-1; k<=row; k++ ) {
			System.out.print("\n$");
		}
	}

}
