package fourth;

public class TenthGrade {

	String name;
	int age;
	int marks;
	
	public void girls() {
		System.out.println(name + "is a best student in the class");
		
	}
	public void girls(int age) {
		
		System.out.println(name + "is " + age + "years old");
		
	}
	public void exam(){
		System.out.println(name + "scored" + marks);
	}
}
