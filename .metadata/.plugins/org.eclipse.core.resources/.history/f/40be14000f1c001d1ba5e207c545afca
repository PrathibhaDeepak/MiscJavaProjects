package arrayPack;

public class ArrBrototype {

	public static void main(String[] args) {
		int num=10;
		int sum=0;
		int count =0;
	
	for(int i=0; i<=10; i++) {
		
		//Sum of prime numbers
		if(num%i==2) {
			count++;	
			
		}
		if(count==2) {
			System.out.printf("Prime Number: %d", num);
	
		}
	}

	}

}
