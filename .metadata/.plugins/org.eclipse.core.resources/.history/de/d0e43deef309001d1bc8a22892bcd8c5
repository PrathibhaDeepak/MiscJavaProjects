package helloLearn;

public class Assessment_Array {

	public static void main(String[] args) {
		int [] ant = {0,1,2,3,4,5};
		
		for(int i=0; i<ant.length; i++) { 
						
			//System.out.print(ant[i] +"\t");
			//System.out.println("\n");
			for(int odd=0; odd<=ant.length; odd++) {
				if(odd%2==1) {
					System.out.println("It is an Even number:" + ant[odd]);
				}
				elseif(odd%2==0);
					
					System.out.println("Odd number:" + ant[odd]);
				 
			}

			
		}
		
}

	private static void elseif(boolean b) {
		// TODO Auto-generated method stub
		
	}

	}
