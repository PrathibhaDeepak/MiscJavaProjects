package arrayPack;

import java.util.Scanner;

public class Table5 {

	public static void main(String[] args) {
		int num;
		//int sum = 1;
		System.out.print("Enter a number: ");
		Scanner input = new Scanner(System.in);
		 num = input.nextInt();
		 input.close();
		 for(int i=1; i<=10; i++) {
			 for(int j=1; j<=i; j++) {
				 num = i*j;
			System.out.printf("\n%d * %d = %d", i,j,num);
			 }
			 
			 
		 }
		

	}

}
