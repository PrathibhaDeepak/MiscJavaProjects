package java1;

public class LoopJava {

	public static void main(String[] args) {
		
		int num = 1000;
		int i;
		
		for(i = 1; i<=num; i++) {
			if((i % 3 == 0)|| (i % 5 == 0) || (i % 7 == 0)){
				System.out.println(i);
				
			}
		}
		
		
		
		 
		
