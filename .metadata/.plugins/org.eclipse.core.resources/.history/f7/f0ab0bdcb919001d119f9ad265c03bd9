package arrayPack;

public class ArrayClass {

	public static void main(String[] args) {
		int count=0;
		for(int i=1; i<=5; i++) {
			if(i%2==0) {
				count++;
				System.out.printf("Even Numbers",i);
			}
			else {
				System.out.printf("Odd Numbers",i);
			}
		}
		System.out.printf("\nNumber of Even numbers are: %d",count);
		System.out.printf("\nNumber of Odd numbers are: %d",count);
	}

}
